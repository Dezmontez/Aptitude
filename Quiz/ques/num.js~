function ques(n)
{
var que = new Array();
switch(n)
{
case 1:
que[0]="What should be the maximum value of B in the following equation? 5A9 - 7B2 + 9C6 = 823";
que[1]="5";
que[2]="6";
que[3]="7";
que[4]="9";
que[5]="7";
break;


case 2:
que[0]="5358 * 51 = ?";
que[1]="273258";
que[2]="273268";
que[3]="273348";
que[4]="273358";
que[5]="273258";
break;


case 3:
que[0]="1307 * 1307 = ?";
que[1]="1601249";
que[2]="1607249";
que[3]="1701249";
que[4]="1708249";
que[5]="1708249";
break;

case 4:
que[0]="1399 * 1399 = ?";
que[1]="1687401";
que[2]="1901541";
que[3]="1943211";
que[4]="1957201";
que[5]="1957201";
break;

case 5:
que[0]="What number should replace x in this multiplication problem ? 3x4 * 4 = 1216";
que[1]="0";
que[2]="2";
que[3]="4";
que[4]="5";
que[5]="0";
break;

case 6:
que[0]="The total number of even prime number is ";
que[1]="0";
que[2]="1";
que[3]="2";
que[4]="none of the above";
que[5]="1";
break;

case 7:
que[0]="The sum of first 45 natural numbers is ";
que[1]="1035";
que[2]="1280";
que[3]="2070";
que[4]="2140";
que[5]="1035";
break;

case 8:
que[0]="the sum of even numbers between 1 and 31 is";
que[1]="16";
que[2]="128";
que[3]="240";
que[4]="512";
que[5]="240";
break;

case 9:
que[0]="A number being successively divided by 3,5,8 leaves remainder 1,4,7 respectively. find the number ";
que[1]="288";
que[2]="258";
que[3]="238";
que[4]="268";
que[5]="238";
break;

case 10:
que[0]="If n is a negative number, then which of the following is the least?";
que[1]="0";
que[2]="-n";
que[3]="2n";
que[4]="n*n";
que[5]="2n";
break;


case 11:
que[0]="If x and y are negative, then which of the following statements is/are always true? 1) x+y is positive 2)xy is positive 3)x-y is positive";
que[1]="1 only";
que[2]="2 only";
que[3]="3 only";
que[4]="none";
que[5]="2 only";
break;


case 12:
que[0]="If a and b are both odd numbers which of the following is an even numbers?";
que[1]="a+b";
que[2]="ab";
que[3]="a+b+1";
que[4]="ab+2";
que[5]="a+b";
break;

case 13:
que[0]="Which of the following is always odd?";
que[1]="Sum of two odd numbers";
que[2]="Difference of two odd numbers";
que[3]="Product of two odd number";
que[4]="None ";
que[5]="Product of two odd number";
break;

case 14:
que[0]="In the following number the place value of 2 is 6892";
que[1]="2";
que[2]="20";
que[3]="25";
que[4]="7";
que[5]="2";
break;


case 15:
que[0]="In the following the face value of 4 is 4590";
que[1]="4";
que[2]="40";
que[3]="400";
que[4]="4000";
que[5]="4000";
break;

case 16:
que[0]="313*313 + 287*287";
que[1]="180338";
que[2]="170338";
que[3]="160338";
que[4]="190338";
que[5]="180338";
break;


case 17:
que[0]="Which is prime number among the following";
que[1]="225";
que[2]="241";
que[3]="391";
que[4]="392";
que[5]="241";
break;

case 18:
que[0]="If ( 1 + 4 + 9 + .... +100) = 385 then the value of ( 4 + 16 + 36+ ... 400) is :";
que[1]="770";
que[2]="1155";
que[3]="1540";
que[4]="385*385";
que[5]="1540";
break;

case 19:
que[0]="Find the total number of prime factors in the expression
4^11 * 7^5 * 11^2";
que[1]="18";
que[2]="29";
que[3]="40";
que[4]="35";
que[5]="29";
break;

case 20:
que[0]="";
que[1]="";
que[2]="";
que[3]="";
que[4]="";
que[5]="";
break;

}
return que;
}
